        <outline text="GeenStijl" htmlUrl="http://www.geenstijl.nl/" xmlUrl="http://www.geenstijl.nl/atom.xml" />
      </outline>
    </outline>
    <outline text="Ongelezen" title="Ongelezen" description="Ongelezen" type="vfolder" htmlUrl="" xmlUrl="vfolder">
      <outline type="rule" text="Read status" rule="unread" value="" additive="true"/>
    </outline>
    <outline text="Gemarkeerd met vlaggetje" title="Gemarkeerd met vlaggetje" description="Gemarkeerd met vlaggetje" type="vfolder" htmlUrl="" xmlUrl="vfolder">
